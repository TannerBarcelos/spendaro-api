{
  "name": "spendaro-api",
  "type": "module",
  "version": "1.0.0",
  "description": "Official Spendaro Budgeting App API Server",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate-migration-dev": "drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:generate-migration-prod": "drizzle-kit generate --config=drizzle-prod.config.ts",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "src/db/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/swagger": "^9.1.0",
    "@scalar/fastify-api-reference": "^1.25.27",
    "@scalar/themes": "^0.9.33",
    "@scalar/types": "^0.0.12",
    "@t3-oss/env-core": "^0.11.1",
    "config": "^3.3.12",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "fastify": "^5.0.0",
    "fastify-api-reference": "link:@types/@scalar/fastify-api-reference",
    "fastify-bcrypt": "^1.0.1",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^4.0.2",
    "fastify-zod": "^1.4.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.4.1",
    "postgres": "^3.4.4",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/config": "^3.3.5",
    "@types/node": "^22.7.4",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "nodemon": "^3.1.7",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}

{
  "server": {
    "port": 3000,
    "api": {
      "version": "v1"
    },
    "logging": {
      "level": "debug"
    },
    "rate_limit": {
      "max": 15,
      "time_window": 10000,
      "global": true,
      "allow_list": [
        "http://localhost:5173",
        "http://0.0.0.0",
        "http://127.0.0.1"
      ]
    },
    "cache": {
      "name": "redis",
      "host": "localhost",
      "port": 6379,
      "connection_timeout": 5000,
      "max_retries": 3
    },
    "upload-thing": {
      "max_file_size": "4mb",
      "max_files": 1
    }
  }
}

{
  "server": {
    "port": 8010,
    "api": {
      "version": "v1"
    },
    "logging": {
      "level": "debug"
    },
    "rate_limit": {
      "max": 3,
      "time_window": 10000,
      "global": true,
      "whitelist": [
        "http://localhost:5173",
        "http://0.0.0.0",
        "http://127.0.0.1"
      ]
    }
  },
  "security": {
    "jwt": {
      "salt_rounds": 10,
      "expiry": "15m",
      "refresh_expiry": "7d"
    }
  }
}
